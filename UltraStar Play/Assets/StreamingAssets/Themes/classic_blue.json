{
    "defaultControl": {
        "fontColor": "#f0feff",
        "backgroundColor": "#42a3e7",
        "hoverBackgroundColor": "#7dc6e9",
        "focusBackgroundColor": "#7dc6e9",
        "activeBackgroundColor": "#3c95d1",
        "focusBorderColor": "#42a3e7",
    },

    "transparentButton": {
        "backgroundColor": "#00000100",
        "hoverBackgroundColor": "#20202060",
        "focusBackgroundColor": "#20202080",
        "activeBackgroundColor": "#40404080",
        "textShadow": {
            "color": "#a0a0a0",
            "offset": {
                "x": 1,
                "y": 1
            }
        }
    },

    "dynamicPanel": {
        "backgroundColor": "#23304300",
        "focusBackgroundColor": "#00a07e"
    },

    "staticPanel": {
        "backgroundGradient": "24deg, #425169, #38465f"
    },
    
    "primaryFontColor": "#f0feff",
    "secondaryFontColor": "#CCCCCC",
