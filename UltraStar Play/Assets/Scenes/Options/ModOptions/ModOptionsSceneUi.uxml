                <ui:VisualElement name="row" style="align-items: center;">
                    <MaterialIcon tabindex="-1" name="warningIcon" icon="warning" class="rounded mr-1" />
                    <ui:Label tabindex="-1" text="$options_mod_securityWarning" parse-escape-sequences="true" display-tooltip-when-elided="true" name="modTrustWarningLabel" class="wordWrap" />
