            </ui:VisualElement>
            <ui:Toggle label="$options_reloadModsOnFileChange" name="modReloadOnChangeToggle" />
            <ui:VisualElement name="modList" />
        </ui:ScrollView>
    </ui:VisualElement>
</ui:UXML>
